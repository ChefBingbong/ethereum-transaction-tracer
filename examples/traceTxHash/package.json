{
  "name": "@evm-tt/txhash",
  "type": "module",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ChefBingbong/ethereum-transaction-tracer.git",
    "directory": "examples/traceTxHash"
  },
  "scripts": {
    "check": "tsc --pretty --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules",
    "test": "vitest run",
    "trace:tx": "bun run ./src/traceTxHash.example.ts",
    "trace:tx-gas": "bun run ./src/traceTxHashGas.example.ts",
    "test:watch": "vitest run",
    "prepare": "lefthook install"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5",
    "@evm-tt/typescript-config": "workspace:*"
  },
  "dependencies": {
    "@evm-tt/tracer": "workspace:*",
    "pino-pretty": "^13.1.1",
    "viem": "^2.33.3",
    "zod": "^4.0.17"
  },
  "overrides": {
    "pino-pretty>strip-json-comments": "3.1.1"
  }
}
