{
  "name": "@evm-transaction-trace/txrequest",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/ChefBingbong/ethereum-transaction-tracer.git",
    "directory": "packages/examples"
  },
  "scripts": {
    "check": "tsc --pretty --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules",
    "test": "vitest run",
    "trace:request": "bun run ./src/traceTxRequest.example.ts",
    "trace:request-gas": "bun run ./src/traceTxRequestGas.example.ts",
    "test:watch": "vitest run",
    "prepare": "lefthook install"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5",
    "@evm-transaction-trace/typescript-config": "workspace:*"
  },
  "dependencies": {
    "@evm-transaction-trace/tracer": "^2.3.0",
    "pino-pretty": "^13.1.1",
    "viem": "^2.33.3",
    "zod": "^4.0.17"
  },
  "overrides": {
    "pino-pretty>strip-json-comments": "3.1.1"
  }
}
