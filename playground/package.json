{
  "name": "@evm-tt/playground",
  "type": "module",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ChefBingbong/ethereum-transaction-tracer.git",
    "directory": "examples/bun/traceRequest"
  },
  "scripts": {
    "check": "tsc --pretty --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules",
    "trace:tx": "bun run ./src/traceTxHash.example.ts",
    "trace:request": "bun run ./src/traceTxRequest.example.ts",
    "prepare": "lefthook install"
  },
  "devDependencies": {
    "@evm-tt/typescript-config": "workspace:*",
    "@types/bun": "latest",
    "@types/fs-extra": "^11.0.4"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@evm-tt/tracer": "workspace:*",
    "pino-pretty": "^13.1.1",
    "viem": "^2.33.3",
    "zod": "^4.0.17"
  },
  "overrides": {
    "pino-pretty>strip-json-comments": "3.1.1"
  }
}
