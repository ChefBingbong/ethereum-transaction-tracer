{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "trace:tx": {
      "persistent": true,
      "cache": false,
      "outputLogs": "none"
    },
    "trace:tx:gas": {
      "persistent": true,
      "cache": false,
      "outputLogs": "none"
    },
    "trace:requests": {
      "persistent": true,
      "cache": false,
      "outputLogs": "none"
    },
    "trace:request:gas": {
      "persistent": true,
      "cache": false,
      "outputLogs": "none"
    },
    "start": {
      "persistent": true,
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "check": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": [],
      "env": []
    }
  }
}
