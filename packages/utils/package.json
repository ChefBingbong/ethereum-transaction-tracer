{
  "name": "@evm-tt/utils",
  "version": "0.10.0",
  "license": "ISC",
  "sideEffects": false,
  "types": "./dist/index.d.ts",
  "files": [
    "src",
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ChefBingbong/ethereum-transaction-tracer.git",
    "directory": "packages/utils"
  },
  "scripts": {
    "check": "tsc --pretty --noEmit",
    "clean": "rm -rf .turbo node_modules dist",
    "build": "tsc",
    "build:cjs": "tsc -p tsconfig.build.json --module commonjs --moduleResolution node --outDir dist/cjs --removeComments --verbatimModuleSyntax false && printf '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "build:esm": "tsc -p tsconfig.build.json --module esnext --moduleResolution node --outDir dist/esm --sourceMap true && printf '{\"type\":\"module\",\"sideEffects\":false}' > dist/esm/package.json",
    "build:types": "tsc -p tsconfig.types.json",
    "prepublishOnly": "bun run build",
    "prepack": "bun ../../.github/replace-workspace-protocol.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@evm-tt/typescript-config": "workspace:*"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^14.0.0",
    "@opentf/cli-pbar": "^0.7.2",
    "abitype": "^1.0.9",
    "chalk": "4",
    "pino": "9.6.0",
    "pino-pretty": "^13.1.1",
    "radash": "^12.1.1",
    "viem": "^2.34.0",
    "zod": "^4.0.17"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "bun": "./src/index.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./data-structures": {
      "types": "./dist/types/data-structures/index.d.ts",
      "bun": "./src/data-structures/index.ts",
      "import": "./dist/data-structures/index.js",
      "default": "./dist/data-structures/index.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=22"
  }
}