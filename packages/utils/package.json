{
  "name": "@evm-tt/utils",
  "version": "0.14.0",
  "license": "ISC",
  "sideEffects": false,
  "main": "./_cjs/index.js",
  "module": "./_esm/index.js",
  "types": "./_types/index.d.ts",
  "typings": "./_types/index.d.ts",
  "files": [
    "*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ChefBingbong/ethereum-transaction-tracer.git",
    "directory": "packages/utils"
  },
  "scripts": {
    "check": "tsc --pretty --noEmit",
    "clean": "rm -rf ./_esm ./_cjs ./_types node_modules",
    "build": "bun build:cjs && bun build:esm && bun build:types",
    "build:cjs": "tsc --project ./tsconfig.json --module commonjs --moduleResolution node --outDir ./_cjs --removeComments --verbatimModuleSyntax false && printf '{\"type\":\"commonjs\"}' > ./_cjs/package.json",
    "build:esm": "tsc --project ./tsconfig.json --outDir ./_esm && printf '{\"type\": \"module\",\"sideEffects\":false}' > ./_esm/package.json",
    "build:types": "tsc --project ./tsconfig.json --declarationDir ./_types --emitDeclarationOnly --declaration --declarationMap",
    "prepublishOnly": "bun run build",
    "prepack": "bun ../../.github/replace-workspace-protocol.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@evm-tt/typescript-config": "workspace:*"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^14.0.0",
    "@opentf/cli-pbar": "^0.7.2",
    "abitype": "^1.0.9",
    "chalk": "4",
    "pino": "9.6.0",
    "pino-pretty": "^13.1.1",
    "radash": "^12.1.1",
    "viem": "^2.34.0",
    "zod": "^4.0.17"
  },
  "exports": {
    ".": {
      "types": "./_types/index.d.ts",
      "bun": "./src/index.ts",
      "import": "./_esm/index.js",
      "default": "./_cjs/index.js",
      "require": "./_esm/index.js"
    },
    "./data-structures": {
      "types": "./_types/data-structures/index.d.ts",
      "bun": "./src/data-structures/index.ts",
      "import": "./_esm/data-structures/index.js",
      "default": "./_cjs/data-structures/index.js",
      "require": "./_esm/data-structures/index.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "data-structures": [
        "./_types/data-structures/index.d.ts"
      ]
    }
  },
  "engines": {
    "node": ">=22"
  }
}