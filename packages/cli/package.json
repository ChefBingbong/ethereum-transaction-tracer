{
  "name": "@evm-tt/cli",
  "version": "0.6.0",
  "license": "ISC",
  "type": "module",
  "bin": {
    "evm-tt": "./dist/index.js"
  },
  "files": [
    "src",
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "check": "tsc --pretty --noEmit",
    "clean": "rm -rf .turbo node_modules dist",
    "build": "bun run clean && bun run build:cjs",
    "build:cjs": "tsc -p tsconfig.json --module commonjs --moduleResolution node --outDir ./dist --rootDir ./src --verbatimModuleSyntax false && printf '{\"type\":\"commonjs\"}' > ./dist/package.json",
    "prepublishOnly": "bun run build",
    "prepack": "bun ../../.github/replace-workspace-protocol.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@clack/core": "^0.5.0",
    "@clack/prompts": "^0.11.0",
    "@commander-js/extra-typings": "^14.0.0",
    "@evm-tt/tracer": "workspace:*",
    "@evm-tt/utils": "workspace:*",
    "@opentf/cli-pbar": "^0.7.2",
    "chalk": "^4",
    "fs-extra": "^11.3.1",
    "gradient-string": "^3.0.0",
    "type-fest": "^4.41.0",
    "viem": "^2.34.0",
    "zod": "^4.1.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@evm-tt/typescript-config": "workspace:*"
  },
  "engines": {
    "node": ">=22"
  }
}
