{
  "name": "@evm-transaction-trace/tracer",
  "version": "2.5.0",
  "type": "module",
  "license": "ISC",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "typings": "./dist/types/index.d.ts",
  "files": [
    "*",
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/ChefBingbong/ethereum-transaction-tracer.git",
    "directory": "packages/tracer"
  },
  "scripts": {
    "check": "tsc --pretty --noEmit ",
    "clean": "rm -rf .turbo node_modules dist",
    "build": "bun run clean && bun run build:cjs && bun run build:esm && bun run build:types",
    "build:cjs": "tsc -p tsconfig.json --module commonjs --moduleResolution node --rootDir ./src --outDir ./dist/cjs --removeComments --verbatimModuleSyntax false && printf '{\"type\":\"commonjs\"}' > ./dist/cjs/package.json",
    "build:esm": "tsc -p tsconfig.json --module esnext --moduleResolution node --rootDir ./src --outDir ./dist/esm && printf '{\"type\": \"module\",\"sideEffects\":false}' > ./dist/esm/package.json",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly --declaration --declarationMap --rootDir ./src --declarationDir ./dist/types",
    "prepublishOnly": "bun run build",
    "prepack": "bun ../../.github/replace-workspace-protocol.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@evm-transaction-trace/typescript-config": "workspace:*"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@evm-transaction-trace/utils": "workspace:*",
    "picocolors": "^1.1.1",
    "viem": "^2.33.3",
    "zod": "^4.0.17"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./cache": {
      "types": "./dist/types/cache/index.d.ts",
      "import": "./dist/esm/cache/index.js",
      "default": "./dist/cjs/cache/index.js"
    },
    "./callTracer": {
      "types": "./dist/types/callTracer/index.d.ts",
      "import": "./dist/esm/callTracer/index.js",
      "default": "./dist/cjs/callTracer/index.js"
    },
    "./decoder": {
      "types": "./dist/types/decoder/index.d.ts",
      "import": "./dist/esm/decoder/index.js",
      "default": "./dist/cjs/decoder/index.js"
    },
    "./format": {
      "types": "./dist/types/format/index.d.ts",
      "import": "./dist/esm/format/index.js",
      "default": "./dist/cjs/format/index.js"
    },
    "./types": {
      "types": "./dist/types/types/index.d.ts",
      "import": "./dist/esm/types/index.js",
      "default": "./dist/cjs/types/index.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "cache": [
        "./dist/types/cache/index.d.ts"
      ],
      "callTracer": [
        "./dist/types/callTracer/index.d.ts"
      ],
      "decoder": [
        "./dist/types/decoder/index.d.ts"
      ],
      "format": [
        "./dist/types/format/index.d.ts"
      ],
      "types": [
        "./dist/types/types/utils.d.ts"
      ]
    }
  },
  "engines": {
    "node": ">=22"
  }
}
