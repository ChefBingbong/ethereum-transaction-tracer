{
  "name": "@evm-tt/tracer",
  "version": "0.12.0",
  "license": "ISC",
  "type": "module",
  "main": "./_cjs/index.js",
  "module": "./_esm/index.js",
  "types": "./_types/index.d.ts",
  "typings": "./_types/index.d.ts",
  "files": [
    "src",
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/ChefBingbong/ethereum-transaction-tracer.git",
    "directory": "packages/tracer"
  },
  "scripts": {
    "check": "tsc --pretty --noEmit",
    "clean": "rm -rf ./_esm ./_cjs ./_types",
    "build": "bun clean && bun build:cjs && bun build:esm && bun build:types",
    "build:cjs": "tsc --project ./tsconfig.json --module commonjs --moduleResolution node --outDir ./_cjs --removeComments --verbatimModuleSyntax false && printf '{\"type\":\"commonjs\"}' > ./_cjs/package.json",
    "build:esm": "tsc --project ./tsconfig.json --outDir ./_esm && printf '{\"type\": \"module\",\"sideEffects\":false}' > ./_esm/package.json",
    "build:types": "tsc --project ./tsconfig.json --declarationDir ./_types --emitDeclarationOnly --declaration --declarationMap",
    "prepublishOnly": "bun run build",
    "prepack": "bun ../../.github/replace-workspace-protocol.ts",
    "test": "vitest run -c ./vitest.config.mjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@evm-tt/typescript-config": "workspace:*",
    "@types/bun": "latest",
    "@types/fs-extra": "^11.0.4"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@evm-tt/utils": "workspace:*",
    "@opentf/cli-pbar": "^0.7.2",
    "@viem/anvil": "^0.0.10",
    "fs-extra": "^11.3.1",
    "picocolors": "^1.1.1",
    "radash": "^12.1.1",
    "viem": "^2.33.3",
    "zod": "^4.0.17"
  },
  "exports": {
    ".": {
      "types": "./_types/index.d.ts",
      "bun": "./src/index.ts",
      "import": "./_esm/index.js",
      "require": "./_esm/cjs/index.js",
      "default": "./_cjs/index.js"
    },
    "./cache": {
      "types": "./_types/cache/index.d.ts",
      "bun": "./src/cache/index.ts",
      "import": "./_esm/cache/index.js",
      "require": "./_esm/cache/index.js",
      "default": "./_cjs/cache/index.js"
    },
    "./callTracer": {
      "types": "./_types/callTracer/index.d.ts",
      "bun": "./src/callTracer/index.ts",
      "import": "./_esm/callTracer/index.js",
      "require": "./_esm/callTracer/index.js",
      "default": "./_cjs/callTracer/index.js"
    },
    "./decoder": {
      "types": "./_types/decoder/index.d.ts",
      "bun": "./src/decoder/index.ts",
      "import": "./_esm/decoder/index.js",
      "require": "./_esm/decoder/index.js",
      "default": "./_cjs/decoder/index.js"
    },
    "./format": {
      "types": "./_types/format/index.d.ts",
      "bun": "./src/format/index.ts",
      "import": "./_esm/format/index.js",
      "require": "./_esm/format/index.js",
      "default": "./_cjs/format/index.js"
    },
    "./types": {
      "types": "./_types/types/index.d.ts",
      "bun": "./src/types/index.ts",
      "import": "./_esm/types/index.js",
      "require": "./_esm/types/index.js",
      "default": "./_cjs/types/index.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "cache": [
        "./_types/cache/index.d.ts"
      ],
      "callTracer": [
        "./_types/callTracer/index.d.ts"
      ],
      "decoder": [
        "./_types/decoder/index.d.ts"
      ],
      "format": [
        "./_types/format/index.d.ts"
      ],
      "types": [
        "./_types/types/index.d.ts"
      ]
    }
  },
  "engines": {
    "node": ">=22"
  }
}