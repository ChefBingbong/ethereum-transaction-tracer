{
  "name": "@evm-transaction-trace/tracer",
  "version": "2.3.0",
  "type": "module",
  "license": "ISC",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/src/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/ChefBingbong/ethereum-transaction-tracer.git",
    "directory": "packages/tracer"
  },
  "scripts": {
    "check": "tsc --pretty --noEmit ",
    "clean": "rm -rf .turbo node_modules dist --outDir dist",
    "build": "bun run build:js && bun run build:types",
    "build:js": "bun run build:esm && bun run build:cjs",
    "build:esm": "bun build ./src/index.ts --outdir ./dist/esm --format esm --target node --external @evm-transaction-trace/utils --external picocolors --external viem --external zod",
    "build:cjs": "bun build ./src/index.ts --outdir ./dist/cjs --format cjs --target node --external @evm-transaction-trace/utils --external picocolors --external viem --external zod",
    "build:types": "tsc -p tsconfig.json --emitDeclarationOnly --declaration --declarationMap false --outDir dist/types --noEmit false",
    "prepublishOnly": "bun run build",
    "prepack": "bun ../../.github/replace-workspace-protocol.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@evm-transaction-trace/typescript-config": "workspace:*"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@evm-transaction-trace/utils": "workspace:*",
    "picocolors": "^1.1.1",
    "viem": "^2.33.3",
    "zod": "^4.0.17"
  },
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.js"
    }
  },
  "engines": {
    "node": ">=22"
  }
}
