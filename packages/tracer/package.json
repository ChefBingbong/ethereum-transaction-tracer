{
  "name": "@evm-tt/tracer",
  "version": "0.11.0",
  "license": "ISC",
  "types": "./dist/index.d.ts",
  "files": [
    "src",
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/ChefBingbong/ethereum-transaction-tracer.git",
    "directory": "packages/tracer"
  },
  "scripts": {
    "check": "tsc --pretty --noEmit",
    "clean": "rm -rf .turbo node_modules dist",
    "build": "tsc",
    "prepublishOnly": "bun run build",
    "prepack": "bun ../../.github/replace-workspace-protocol.ts",
    "test": "vitest run -c ./vitest.config.mjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@evm-tt/typescript-config": "workspace:*",
    "@types/bun": "latest",
    "@types/fs-extra": "^11.0.4"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@evm-tt/utils": "0.11.0",
    "@opentf/cli-pbar": "^0.7.2",
    "@viem/anvil": "^0.0.10",
    "fs-extra": "^11.3.1",
    "picocolors": "^1.1.1",
    "radash": "^12.1.1",
    "viem": "^2.33.3",
    "zod": "^4.0.17"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "bun": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/index.js"
    },
    "./cache": {
      "types": "./dist/cache/index.d.ts",
      "bun": "./src/cache/index.ts",
      "import": "./dist/cache/index.js",
      "default": "./dist/cache/index.js"
    },
    "./callTracer": {
      "types": "./dist/callTracer/index.d.ts",
      "bun": "./src/callTracer/index.ts",
      "import": "./dist/callTracer/index.js",
      "default": "./dist/callTracer/index.js"
    },
    "./decoder": {
      "types": "./dist/decoder/index.d.ts",
      "bun": "./src/decoder/index.ts",
      "import": "./dist/decoder/index.js",
      "default": "./dist/decoder/index.js"
    },
    "./format": {
      "types": "./dist/format/index.d.ts",
      "bun": "./src/format/index.ts",
      "import": "./dist/format/index.js",
      "default": "./dist/format/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "bun": "./src/types/index.ts",
      "import": "./dist/types/index.js",
      "default": "./dist/types/index.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=22"
  }
}
