{
  "name": "@evm-tt/tracer",
  "version": "0.10.2",
  "license": "ISC",
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "typings": "./dist/types/index.d.ts",
  "files": [
    "src",
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/ChefBingbong/ethereum-transaction-tracer.git",
    "directory": "packages/tracer"
  },
  "scripts": {
    "check": "tsc --pretty --noEmit",
    "clean": "rm -rf .turbo node_modules dist",
    "build": "bun run clean && bun run build:cjs && bun run build:esm && bun run build:types",
    "build:cjs": "tsc -p tsconfig.build.json --module commonjs --moduleResolution node --outDir dist/cjs --removeComments --verbatimModuleSyntax false && printf '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "build:esm": "tsc -p tsconfig.build.json --module esnext --moduleResolution node --outDir dist/esm --sourceMap true && printf '{\"type\":\"module\",\"sideEffects\":false}' > dist/esm/package.json",
    "build:types": "tsc -p tsconfig.types.json",
    "prepublishOnly": "bun run build",
    "prepack": "bun ../../.github/replace-workspace-protocol.ts",
    "test": "vitest run -c ./vitest.config.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@evm-tt/typescript-config": "workspace:*",
    "@types/bun": "latest",
    "@types/fs-extra": "^11.0.4"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@evm-tt/utils": "0.10.0",
    "@opentf/cli-pbar": "^0.7.2",
    "@viem/anvil": "^0.0.10",
    "fs-extra": "^11.3.1",
    "picocolors": "^1.1.1",
    "viem": "^2.33.3",
    "zod": "^4.0.17"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "bun": "./src/index.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    },
    "./cache": {
      "types": "./dist/types/cache/index.d.ts",
      "bun": "./src/cache/index.ts",
      "import": "./dist/esm/cache/index.js",
      "require": "./dist/cjs/cache/index.js",
      "default": "./dist/esm/cache/index.js"
    },
    "./callTracer": {
      "types": "./dist/types/callTracer/index.d.ts",
      "bun": "./src/callTracer/index.ts",
      "import": "./dist/esm/callTracer/index.js",
      "require": "./dist/cjs/callTracer/index.js",
      "default": "./dist/esm/callTracer/index.js"
    },
    "./decoder": {
      "types": "./dist/types/decoder/index.d.ts",
      "bun": "./src/decoder/index.ts",
      "import": "./dist/esm/decoder/index.js",
      "require": "./dist/cjs/decoder/index.js",
      "default": "./dist/esm/decoder/index.js"
    },
    "./format": {
      "types": "./dist/types/format/index.d.ts",
      "bun": "./src/format/index.ts",
      "import": "./dist/esm/format/index.js",
      "require": "./dist/cjs/format/index.js",
      "default": "./dist/esm/format/index.js"
    },
    "./types": {
      "types": "./dist/types/types/index.d.ts",
      "bun": "./src/types/index.ts",
      "import": "./dist/esm/types/index.js",
      "require": "./dist/cjs/types/index.js",
      "default": "./dist/esm/types/index.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "cache": [
        "./dist/types/cache/index.d.ts"
      ],
      "callTracer": [
        "./dist/types/callTracer/index.d.ts"
      ],
      "decoder": [
        "./dist/types/decoder/index.d.ts"
      ],
      "format": [
        "./dist/types/format/index.d.ts"
      ],
      "types": [
        "./dist/types/types/index.d.ts"
      ]
    }
  },
  "engines": {
    "node": ">=22"
  }
}